{
	"name": "audioplayer",
	"productName": "Audio Player",
	"version": "1.0.5",
	"description": "Basic Desktop audio player using Electron",
	"main": "src/index.js",
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"test": "echo \"Error: no test specified\" && exit 0",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --windows",
		"build": "electron-builder build -w"
	},
	"keywords": [],
	"author": {
		"name": "Kevin Long",
		"email": "kevin.uu.long@gmail.com"
	},
	"license": "MIT",
	"build": {
		"appId": "com.kevinuulong.audioplayer",
		"fileAssociations": [
			{
				"ext": "mp3"
			},
			{
				"ext": "wav"
			},
			{
				"ext": "ogg"
			},
			{
				"ext": "m4a"
			}
		],
		"files": ["!music/*", "!dist/*"]
	},
	"dependencies": {
		"howler": "^2.1.2",
		"music-metadata": "^7.8.1"
	},
	"devDependencies": {
		"electron": "18.3.7",
		"electron-builder": "^22.11.7",
		"electron-reload": "^1.5.0"
	}
}
